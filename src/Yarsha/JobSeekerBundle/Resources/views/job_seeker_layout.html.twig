        modal.find('.modal-title').html(title);
//        modal.find('.modal-dialog').css({'width': '60%'});

        $.ajax({
            url: '{{ url('yarsha_ajax_job_seeker_general_information') }}',
            method: 'GET',
            success: function (data) {
                modal.find('.modal-body').html(data.template)
            }
        });

    });

    function submitGeneralInformation(obj) {
        $.LoadingOverlay('show');
        var form = $(obj);
        var dataToSend = form.serialize();
        var url = "{{ path('yarsha_ajax_job_seeker_general_information') }}";
        $.ajax({
            type: "POST",
            url: url,
            data: dataToSend,
            success: function (res) {
                $.LoadingOverlay('hide');
                window.location = ''
            },
            error: function (res) {
                $.LoadingOverlay('hide');
               // alert('Fail');
            }
        });
        return false;

    }


