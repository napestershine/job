<script>


    $('#updateProfile').on('show.bs.modal', function (e) {
        var modal = $(this);
        var button = $(e.relatedTarget);
        var title = button.attr('data-title');
        modal.find('.modal-title').html(title);
//        modal.find('.modal-dialog').css({'width': '60%'});

        $.ajax({
            url: '{{ url('yarsha_ajax_job_seeker_updateProfile') }}',
            method: 'GET',
            success: function (data) {
                modal.find('.modal-body').html(data.template);
                loadProfileForm(modal);
            }
        });

    });

    function loadProfileForm(modal) {
        modal.find('.modal-body form').on('submit', function (e) {
            e.preventDefault();
            submitUpdateProfile($(this), modal);
        });
    }

    function submitUpdateProfile(obj, modal) {
        $.LoadingOverlay('show');
        var form = $(obj);
        var dataToSend = form.serialize();
        var url = "{{ path('yarsha_ajax_job_seeker_updateProfile') }}";
        $.ajax({
            type: "POST",
            url: url,
            data: dataToSend,
            success: function (res) {
                if (res.success && res.success == true) {
                    window.location = ''
                } else {
                    modal.find('.modal-body').html(res.template);
                    loadProfileForm(modal);
                }
                $.LoadingOverlay('hide');
            },
            error: function (xhr, status, erro) {
                $.LoadingOverlay('hide');
            }
        });
        return false;

    }

    $('#addEducation').on('show.bs.modal', function (e) {
        var modal = $(this);
        var button = $(e.relatedTarget);
        var companyId = button.attr('data-companyId');
        var title = button.attr('data-title');
        var educationId = button.attr('data-educationId');

        if (educationId != '') {
            var url = "{{ path('yarsha_job_seeker_ajax_update_education', {'educationId':'EDUCATION_ID'}) }}";
            var url = url.replace('EDUCATION_ID', educationId);
        } else {
            var url = "{{ path('yarsha_job_seeker_ajax_add_education') }}";
        }

        modal.find('.modal-title').html(title);

        $.ajax({
            type: 'get',
            url: url,
            data: {'educationId': educationId},
            success: function (res) {
                modal.find('.modal-body').html(res.template);
            },
            error: function (res) {
                alert(res.message);
            }
        });
    });
    function submitEducation(obj) {
        $.LoadingOverlay('show');
        var form = $(obj),
            educationid = form.data('id'),
            dataToSend = form.serialize();

        if (educationid == '') {
            var url = "{{ path('yarsha_job_seeker_ajax_add_education') }}"
        } else {
            var url = "{{ path('yarsha_job_seeker_ajax_update_education', {'educationId':'EDUCATION_ID'}) }}";
            var url = url.replace('EDUCATION_ID', educationid);
        }
        $.ajax({
            type: "POST",
            url: url,
            data: dataToSend,
            success: function (res) {
                $.LoadingOverlay('hide');
                window.location = ''
            },
            error: function (res) {
                alert('fail');
                $.LoadingOverlay('hide');
            }
        });
        return false;
    }

    function deleteEducation(e, obj) {
        e.preventDefault();
        c = confirm('Are you sure ? ');
        if (c) {
            $.LoadingOverlay('show');
            id = $(obj).attr('data-educationId');
            url = "{{ path('yarsha_ajax_seeker_education_delete',{'id':'EDUCATION_ID'}) }}";
            url = url.replace('EDUCATION_ID', id);
            console.log(url)
            $.ajax({
                url: url,
                method: 'GET',
                success: function (data) {
                    $.LoadingOverlay('hide');
                    if (data.success) {
                        $(obj).parent().parent().remove();
                    }
                },
                error:function(res){
                    $.LoadingOverlay('hide');
                    alert('Fail');
                }
            })
        }

    }


